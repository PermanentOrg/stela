{
	"name": "@stela/s3-utils",
	"version": "1.0.0",
	"description": "Helper functions for working with S3, used by Permanent's backend services",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PermanentOrg/stela.git"
	},
	"author": "Permanent.org",
	"license": "AGPL-3.0",
	"bugs": {
